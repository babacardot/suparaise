    "start": "next start",
    "lint": "next lint",
    "agent:run": "ts-node src/agent/index.ts",
    "format": "prettier --write .",
    "update:types": "pnpm exec supabase gen types typescript --project-id swhlekigmfdbwulrncot > src/lib/types/database.ts && pnpm run format",
    "seed:templates": "pnpm exec tsx src/lib/scripts/seed-data/generate-templates.ts",
    "seed:bulk": "pnpm exec tsx src/lib/scripts/seed-data/bulk-seed.ts --format excel",
    "seed:import": "pnpm exec tsx src/lib/scripts/seed-data/import-data.ts",
    "seed:validate-env": "pnpm exec tsx src/lib/scripts/seed-data/validate-env.ts",
    "seed:import-targets": "pnpm exec tsx src/lib/scripts/seed-data/import-data.ts --table targets --file src/lib/scripts/seed-data/production/targets.xlsx",
    "seed:import-angels": "pnpm exec tsx src/lib/scripts/seed-data/import-data.ts --table angels --file src/lib/scripts/seed-data/production/angels.xlsx",
    "seed:import-accelerators": "pnpm exec tsx src/lib/scripts/seed-data/import-data.ts --table accelerators --file src/lib/scripts/seed-data/production/accelerators.xlsx",
